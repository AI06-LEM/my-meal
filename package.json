{
    "name": "my-meal",
    "version": "1.0.0",
    "description": "School restaurant meal planning application using native Node.js SQLite",
    "main": "server.js",
    "scripts": {
        "check:env": "node scripts/preinstall.js",
        "start": "node --experimental-sqlite server.js",
        "dev": "nodemon --exec 'node --experimental-sqlite' server.js",
        "test": "cucumber-js",
        "test:smoke": "cucumber-js --profile smoke",
        "test:happy": "cucumber-js --profile happy",
        "test:guest": "cucumber-js --profile guest",
        "test:admin": "cucumber-js --profile admin",
        "test:wip": "cucumber-js --profile wip",
        "test:ci": "cucumber-js --profile ci",
        "test:debug": "HEADLESS=false cucumber-js --profile debug",
        "test:tag": "cucumber-js --tags",
        "test:tagged_debug": "HEADLESS=false cucumber-js --tags"
    },
    "engines": {
        "node": ">=22.5.0",
        "npm": ">=8.0.0"
    },
    "dependencies": {
        "cors": "^2.8.5",
        "express": "^4.18.2",
        "multer": "^1.4.5-lts.1"
    },
    "devDependencies": {
        "@cucumber/cucumber": "^12.5.0",
        "@playwright/test": "^1.57.0",
        "nodemon": "^3.0.1",
        "playwright": "^1.57.0"
    },
    "keywords": [
        "meal",
        "planning",
        "restaurant",
        "school",
        "sqlite",
        "native"
    ],
    "author": "",
    "license": "MIT"
}